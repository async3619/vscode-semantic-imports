{
  "name": "vscode-semantic-imports",
  "displayName": "Semantic Imports",
  "description": "Accurate syntax highlighting for imported symbols in TypeScript",
  "version": "1.1.2",
  "publisher": "async3619",
  "license": "MIT",
  "icon": "assets/icon.png",
  "packageManager": "pnpm@10.29.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/async3619/vscode-semantic-imports"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "extensionKind": [
    "workspace"
  ],
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:typescript",
    "onLanguage:typescriptreact"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "typescriptServerPlugins": [
      {
        "name": "semantic-imports-ts-plugin",
        "enableForWorkspaceTypeScriptVersions": true
      }
    ]
  },
  "scripts": {
    "build": "node esbuild.mjs",
    "watch": "node esbuild.mjs --watch",
    "lint": "eslint",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "package": "pnpm run build && vsce package --no-dependencies",
    "publish": "pnpm run build && vsce publish --no-dependencies",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.6",
    "@semantic-release/npm": "^13.1.4",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/node": "^20.11.0",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^2.1.9",
    "@vscode/vsce": "^3.0.0",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "esbuild": "^0.24.0",
    "esbuild-plugin-tsc": "^0.5.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "husky": "^9.1.7",
    "prettier": "^3.4.0",
    "semantic-release": "^25.0.3",
    "typescript": "^5.7.0",
    "unplugin-swc": "^1.5.9",
    "vitest": "^2.1.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "@vscode/vsce-sign",
      "keytar",
      "@swc/core"
    ]
  },
  "dependencies": {
    "fast-plist": "^0.1.3",
    "inversify": "^7.11.0",
    "jsonc-parser": "^3.3.1",
    "p-queue": "^8.1.1",
    "reflect-metadata": "^0.2.2"
  }
}
